/**app.wxss**/
@import "/pages/outerFile/wxParse/wxParse.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
button{
  background-color: #fff;
}
button::after{
  border: none;
}
div,input,span,a{box-sizing: border-box;-moz-box-sizing:border-box; /* Firefox */-webkit-box-sizing:border-box; /* Safari */}
ul,li{list-style-type:none;}
a{text-decoration:none;}
.fl{float: left;}
.rt{float: right;}
.tc{text-align: center;}
.clearfix:after{visibility:hidden;display:block; font-size:0; content:'\20'; clear:both; height:0;}

.ver-middle{position: absolute; bottom: 50%;left: 50%;transform: translate(-50%, -50%);}
.ft15{font-size:1.5rem;}

.over-circle{display:inline-block;padding:0.1rem;overflow: hidden;text-overflow: ellipsis;    white-space: nowrap;vertical-align: middle;}
.overtwo-circle{display: -webkit-box;overflow: hidden;word-break: break-all;-webkit-line-clamp:2; -webkit-box-orient: vertical;}
.over-circle3{display: -webkit-box;overflow: hidden;word-break: break-all;-webkit-line-clamp:3; -webkit-box-orient: vertical;}
.flex-box{
  display: -webkit-box;
    display: -moz-box; 
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    }
.flex-item1{
  flex-grow:1;
}
.betweenplace{ 
    /*display:-webkit-box;
    display:-moz-box;
    display:box; 
    display:-webkit-flex;
    display:flex;*/
    display: -webkit-box;
    display: -moz-box; 
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    
    -webkit-box-pack:justify;
    -webkit-box-pack: space-between;
    -moz-justify-content: space-between;
    -webkit-justify-content: space-between;
    justify-content:space-between;   
}
.flex-list{width: 25%;}
.commonico{position: relative;}
/*background-size: 1500rpx 750rpx;  */
.commonico:after,.commonico:before{position: absolute;content:'';background-image: url("http://wap.wangxiao.cn/ztkxcx/images/icon3.png?na");background-repeat: no-repeat;background-size: 1800rpx 900rpx;}
.commonico-box{
   background-size: 1800rpx 900rpx;
  background-image: url("http://wap.wangxiao.cn/ztkxcx/images/icon3.png?fh");background-repeat: no-repeat;
}
/* //渐变色 */
.lineargrad{ 
    background-image: -webkit-linear-gradient(top, #2f7aee 0%,#fff 100%);
}
.box-shadow{
  box-shadow: 4rpx 4rpx 20rpx #ccc;
}
.fontc6{
  color: #666;
  font-size: 24rpx;
}
.fontc9{
  color: #999;
  font-size: 24rpx;
}
.fontc{
  color: #666;
}
.fontc3{
  color: #333;
}
.fontc324{
   color: #333;
   font-size: 24rpx;
}
.fonts24{
  font-size: 24rpx;
}
.fontcbl{
  color: #58a6f8;
}
.text-item.act{
  color:#58a6f8;
  border-bottom:4rpx solid #58a6f8;
  
}
.fonts24{font-size: 24rpx;}
.fontsize28{font-size: 28rpx;}
.course-fontyell{
  color: #ff8500;
  font-size: 28rpx;
}
.chapter-list-box{
  padding: 30rpx;
}
.flex-between{
  justify-content:space-between;
   align-items:  flex-end;
}
.flex-between-middle{
   justify-content:space-between;
   align-items: center;
}
.flex-top{
  justify-content:space-between;
  
}
.common-btn{
  background-color:#58a6f8;
  color:#fff;

}
.box-pa{
  padding:0 30rpx;
}
.bg-opa{
    padding: 0rem 1rem;
    z-index: 10031;
    position: fixed;
    background-color: rgba(0, 0, 0, .7);
    width: 100%;
    height: 100%;
    top: 0;
}
.exam_list_box {
   
    background-color: #fff;
    border-radius: 8px;
   
    color: #fff;
    position: fixed;
    top: 50%;
    left: 50%;
    -ms-transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
/*星星相关  */
.star-box {
    margin: 1px 5px 0px 0px;
    width: 75px;
    height: 13px;
    position: relative;
    overflow: hidden;
    background: #ddd;
}
.star-img {
    display: block;
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    vertical-align: middle;
}
.star {
    height: 13px;
    background: orange;
}
/*答案相关  */
.ti-btn.act{
  background-color:#58a6f8;
  color: #fff;
}
.ti-btn.right-ti{
  background-color:#6fd89c;
  color: #fff;
}
.ti-btn.wrong-ti{
  background-color:#f46464;
  color: #fff;
}
/*关闭按钮  */
.circle_clear {
    font-size: 35rpx;
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 50%;
    background-color: #999;
    top:-30rpx;
    right: -15px;
    text-align: center;
    color: #fff;
}
.footer-btn{
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #58a6f8;
  color: #fff;
  z-index: 1000;
}
.footer-item{
  float: left;
  width: 50%;
  text-align: center;
  padding: 25rpx 0;
  border-right: 1px solid #fff;
  font-size: 34rpx;
}
/*暂无数据  */
.img-class{
  width: 370rpx;
  height: 201rpx;
  margin-bottom: 100rpx;
}
.nodata-box{
  top:35%;
  font-size: 40rpx;
  color: #333;
}
.downapp_list_box{
    width:85%;
    padding:0 10rpx;
    border-radius:8px;
    color:#fff;
    position:fixed;
    z-index: 1300;
    top:50%;
    left:50%;
    -ms-transform:translate(-50%, -50%);
    -webkit-transform:translate(-50%, -50%);
    -o-transform:translate(-50%, -50%);
    -moz-transform:translate(-50%, -50%);
    transform:translate(-50%, -50%);
}
image.downapp-bgimg{
  width: 100%;
  height: 650rpx;
}

.app-img{
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  top: 50rpx;
  left:50%;
    -ms-transform:translate(-50%, 0);
    -webkit-transform:translate(-50%,0);
    -o-transform:translate(-50%, 0);
    -moz-transform:translate(-50%, 0);
    transform:translate(-50%, 0);
}
.down-app-title{
  color: #333;
  font-size: 28rpx;
  padding-bottom: 20rpx;
}
.app-name{
  line-height: 45rpx;
}
.app-name,.down-process{
  color: #ff8500;
}
.down-process{
  font-size: 24rpx;
}
.box-bottom{
  position: absolute;
  left: 0;
  width: 100%;
  bottom: 0;
  top: 330rpx;
  box-sizing: border-box;
  padding: 0 60rpx;
}
.down-process{
  width:100%;
  text-align:left;
  line-height:40rpx;
}
