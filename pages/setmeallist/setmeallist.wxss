setmeal-listscroll-view{
  position: relative;
  height: 100%;
  overflow-y: scroll;
}
.view-center{padding: 0rpx 25rpx;padding-top: 25rpx;}
.setmeal-list{
  padding: 20rpx 18rpx 0 18rpx;
  border:1rpx solid #f2f2f2;
  box-shadow:0px 1px 2px #999999;
  margin-bottom:20rpx;
  border-radius: 5rpx;
  flex-direction: column;
}
.setmeal-item-content{
  justify-content: flex-start;
}
.setmeallist-topleft{
  width: 306rpx;
  height: 180rpx;
  flex-shrink:0;
}
.setmeallist-topleft image{
  width: 306rpx;
  height: 180rpx;
}
.setmeallist-topright{
  flex-direction: column;
  flex-direction: -webkit-column;
  justify-content: space-between;
  justify-content: -webkit-between;
  padding:0 20rpx;
}
.setmeal-title{
  font-size: 24rpx;
}
.setmeal-tags{
  flex-flow:row wrap;
}
.setmeal-itemtag-plus{
  border: 1px solid #83C23D;
  color: #83C23D;
  padding: 6rpx 20rpx;
  border-radius: 5px;
  margin:8rpx 10rpx;
  font-size:30rpx;
}
.setmeal-itemtag-all{
  border: 1px solid #FF8500;
  color: #FF8500;
  padding: 6rpx 20rpx;
  border-radius: 5px;
  margin-right: 20rpx;
  font-size:30rpx;
}
.br{
  height: 1px;
  background: #f2f2f2;
  margin-top: 18rpx;
}
.setmeal-list-bottom{
  justify-content:space-between;
  align-items: center;
  height: 68rpx;
  line-height: 68rpx;
  color: #999999;
  font-size: 24rpx;
}
.setmeal-enrolment{
  color: #FF8500;
}
.setmeal-price{
  color: #E93A3F;
  padding-right: 10rpx;
  font-size: 28rpx;
}
.baseline{
  font-size: 30rpx;
  line-height: 30rpx;
  text-align: center;
  padding-bottom:20rpx;
  color: #999999;
}
.no-data{
  position: absolute;
  width: 100%;
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color:#F5F6F7; 
  z-index: 0;
}
.no-data-img{
  width: 371rpx;
  height: 202rpx;
}
.no-data-text{
  margin-top: 86rpx;
  color: #D5D5D5;
}